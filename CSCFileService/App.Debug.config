<?xml version="1.0" encoding="utf-8"?>
<!--For more information on using transformations see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<connectionStrings xdt:Transform="Replace">
		<add name="EDIEntities"
			 connectionString="metadata=res://*/EDI.csdl|res://*/EDI.ssdl|res://*/EDI.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=mfb-us-dsql-001.onling.com;initial catalog=EDI;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;"
			 providerName="System.Data.EntityClient"/>
		<add name="ESPEntities"
			 connectionString="metadata=res://*/ESP.csdl|res://*/ESP.ssdl|res://*/ESP.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=mfb-us-dsql-001.onling.com;initial catalog=ESP_live;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;"
			 providerName="System.Data.EntityClient"/>
	</connectionStrings>
	<log4net>
		<root>
			<level value="DEBUG" xdt:Transform="Replace"/>
		</root>
	</log4net>
	<applicationSettings xdt:Transform="Replace">
		<CSCFileService.Properties.Settings>
			<setting name="FilePatternToLookAt" serializeAs="String">
				<value>*.txt</value>
			</setting>
			<setting name="SMTPPort" serializeAs="String">
				<value>25</value>
			</setting>
			<setting name="SMTPUser" serializeAs="String">
				<value />
			</setting>
			<setting name="SMTPPassword" serializeAs="String">
				<value />
			</setting>
			<setting name="FilesToImportAtOnce" serializeAs="String">
				<value>25</value>
			</setting>
			<setting name="EmailCopy" serializeAs="String">
				<value>True</value>
			</setting>
			<setting name="PurgeFilesDays" serializeAs="String">
				<value>7</value>
			</setting>
			<setting name="CycleTimeMinutes" serializeAs="String">
				<value>1</value>
			</setting>
			<setting name="Emails" serializeAs="String">
				<value>steve.ling@sflservicesllc.com</value>
			</setting>
			<setting name="SMTPFROMEMAIL" serializeAs="String">
				<value>sales@myfflbook.com</value>
			</setting>
			<setting name="SMTPSERVER" serializeAs="String">
				<value>myfflbook-com.mail.protection.outlook.com</value>
			</setting>
			<setting name="PickUpFolder" serializeAs="String">
				<value>c:\Interface\Ord</value>
